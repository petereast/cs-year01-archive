package lab3;

public class Arrays {

	public static int[] reverse (int[] A)
	{
		int[] temp = new int[A.length];
		int i = 1;
		for(int x: A){
			temp[A.length - i] = x;
			i++;
		}
		return temp;
	}
	
	public static void reverse2(int[] A){
		int[] B = A;
		int i;
		for(i = A.length; i > 1; i--){
			A[i] = B[B.length-i];
		}
	}
}
